{
  "$schema": "https://turborepo.dev/schema.json",
  "ui": "tui",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "inputs": ["$TURBO_DEFAULT$", ".env*"],
      "outputs": [".svelte-kit/**", ".vercel/**", "dist/**", "build/**"]
    },
    "lint": {},
    "lint:fix": {},
    "format": {},
    "check": {},
    "fix": {},
    "check:types": {},
    "test": {},
    "test:unit": {},
    "test:e2e": {},
    "dev": {
      "cache": false,
      "persistent": true
    }
  },
  "globalDependencies": ["**/.env.*local"],
  "globalEnv": [
    "NODE_ENV",
    "DATABASE_URL",
    "MODE",
    "API_URL",
    "ADMIN_EMAIL",
    "ADMIN_PASSWORD"
  ]
}
