{
  "$schema": "https://json.schemastore.org/tsconfig",
  "extends": [
    "@repo/typescript-config/nest.json"
  ],
  "compilerOptions": {
    "outDir": "./dist",
    "baseUrl": "./",
    "paths": {
      "$src/*": [
        "./src/*"
      ],
      "$common/*": [
        "./src/common/*"
      ]
    },
    "esModuleInterop": true
  }
}
